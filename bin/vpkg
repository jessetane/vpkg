#
# vpkg
#

# go to own dir
cd "$(dirname $BASH_SOURCE)" && cd "$(pwd)"

# get deps
. ../lib/functions

# only run supported commands
cmd="$1"
if [ "$cmd" == "install" ] ||
   [ "$cmd" == "uninstall" ] ||
   [ "$cmd" == "link" ] ||
   [ "$cmd" == "unlink" ] ||
   [ "$cmd" == "use" ] ||
   [ "$cmd" == "unuse" ]
then
  "$cmd" "${@:2}"
else
  echo "$cmd: command not found" >&2 && exit 1
fi
